rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
      allow read: if request.auth.uid == userId;
    }
    match /users/{userId}/doctors/{doctorId} {
      allow read: if request.auth.uid == userId;
    }
    match /users/{userId}/pacients/{pacientId} {
      allow read: if request.auth.uid == userId;
    }
  }
}